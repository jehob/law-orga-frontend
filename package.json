    "mime": "^2.4.6",
    "moment": "^2.27.0",
    "ng-material-multilevel-menu": "^4.12.2",
    "ngx-cookie-service": "^2.4.0",
    "ngx-quill": "^12.0.1",
    "object-hash": "^2.0.3",
    "quill": "^1.3.7",
    "quill-better-table": "^1.2.10",
    "quill-mention": "^2.2.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
