{
  "name": "rlcapp",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:dev": "lite-server --baseDir=\"./dist\"",
    "build:dev": "ng build --watch",
    "build:public-dev": "ng build --configuration=development",
    "build:local-prod": "ng build --configuration=local-production --watch",
    "deploy:dev": "ng build --configuration=development && env-cmd -f src/environments/dev.config.env node deploy_script.js",
    "deploy:prod": "ng build --configuration=production && env-cmd -f src/environments/prod.config.env node deploy_script.js",
    "deploy:test": "ng build --configuration=test && env-cmd -f src/environments/test.config.env node deploy_script.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/forms": "^9.1.12",
    "@angular/localize": "^9.1.12",
    "@angular/material": "^9.2.4",
    "@angular/material-moment-adapter": "^9.2.4",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "aws-sdk": "^2.534.0",
    "core-js": "^3.0.1",
    "env-cmd": "^10.0.1",
    "file-saver": "^2.0.1",
    "fstream": "^1.0.12",
    "logrocket": "^0.6.21",
    "logrocket-ngrx": "^0.1.1",
    "mime": "^2.4.4",
    "moment": "^2.24.0",
    "ng-material-multilevel-menu": "^4.9.6",
    "ngx-cookie-service": "^2.2.0",
    "object-hash": "^2.0.3",
    "rxjs": "^6.6.2",
    "tar": "^4.4.10",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@ngrx/store-devtools": "^8.6.0",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lite-server": "^2.5.4",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "protractor": "^5.4.2",
    "ts-node": "~8.0.3",
    "tslint": "~5.15.0",
    "typescript": "^3.8.3"
  }
}
