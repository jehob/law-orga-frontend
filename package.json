{
  "name": "rlcapp",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:dev": "lite-server --baseDir=\"./dist\"",
    "build:dev": "ng build --watch",
    "build:public-dev": "ng build --configuration=development",
    "build:local-prod": "ng build --configuration=local-production --watch",
    "deploy:dev": "ng build --configuration=development && env-cmd -f src/environments/dev.config.env node deploy_script.js",
    "deploy:prod": "ng build --configuration=production && env-cmd -f src/environments/prod.config.env node deploy_script.js",
    "deploy:test": "ng build --configuration=test && env-cmd -f src/environments/test.config.env node deploy_script.js"
  },
  "private": true,
  "dependencies": {
